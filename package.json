{
  "name": "react-redux-isomorphic-starterkit",
  "version": "1.0.0",
  "description": "An isomorphic React boilerplate with Redux, Webpack (SASS/CSS loader, JSX/JS loader, File loader...), React Router, React Hot Loader and ESLint static code verification.",
  "scripts": {
    "postinstall": "npm run deploy",
    "clean": "node ./bin/clean.js",
    "server": "node ./bin/server",
    "build": "npm run clean && node ./bin/bundler/prod.js",
    "deploy": "npm run build && npm run server",
    "watch": "npm run clean && node ./bin/bundler/dev.js && concurrent \"node ./bin/server/hotReload.js\" \"nodemon ./bin/server --watch dist -e js\" --kill-others"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayoubdev/react-redux-isomorphic-starterkit.git"
  },
  "bugs": {
    "url": "https://github.com/ayoubdev/react-redux-isomorphic-starterkit/issues"
  },
  "keywords": [
    "react",
    "redux",
    "webpack",
    "react-router",
    "boilerplate",
    "starter",
    "isomorphic",
    "kit",
    "starterkit",
    "starter-kit"
  ],
  "author": "Ayoub ADIB <ayoub.development@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0",
    "del": "^2.2.2",
    "eslint": "^3.8.0",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "node-sass": "^3.10.1",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.9"
  },
  "dependencies": {
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^3.0.0-beta.1"
  }
}
